#!/usr/local/ActivePerl-5.6/bin/cpr
int main(void) {
    CPR_eval("use Inline (C => q{
        char* greet() {
            return \"Hello world\";
        }
    })");
    printf("%s, I'm running under Perl version %s\n",
           CPR_eval("&greet"),
           CPR_eval("use Config; $Config{version}"));
    return 0;
}
